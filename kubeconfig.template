apiVersion: v1
clusters:
- cluster:
    insecure-skip-tls-verify: true
    server: $KUBERNETES_MASTER
  name: bmcs-k8s-cluster
contexts:
- context:
    cluster: bmcs-k8s-cluster
    user: $KUBERNETES_USERNAME
  name: bmcs-k8s-context
current-context: bmcs-k8s-context
kind: Config
preferences: {}
users:
- name: $KUBERNETES_USERNAME
  user:
    client-certificate-data: $KUBECONFIG_CLENT_CERTIFICATE
    client-key-data: $KUBECONFIG_CLENT_KEY
